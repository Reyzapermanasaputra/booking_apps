<!DOCTYPE html>
<html>
<head>
  <title>BookingApps</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class="login">
<div class="container">
  	<% flash.each do |key, value| %>
      <div id="hidden" class="alert <%= flash_class key %> alert-dismissable">
        <button id="thebutton" type="button" class="close" data-dismiss="alert">&times;</button>
  	    <p> <%= value %> </p>
  	  </div>
    <% end %>
<%= yield %>

</body>
</html>
