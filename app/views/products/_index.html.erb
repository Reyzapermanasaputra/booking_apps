<% @products.each_with_index do |identity, index| %>
  <tr>
    <td><%= index+1 %></td>
  	<td><%= identity.nama_lembaga %></td>
  	<td><%= identity.alamat_lengkap %></td>
  	<td><%= identity.desa %></td>
  	<td><%= identity.kecamatan %></td>
  	<td><%= identity.kabupaten %></td>
  	<td><%= identity.no_akte %></td>
  	<td><%= link_to "Edit", edit_product_path(identity), remote: true, class: "btn btn-default" %></td>
  	<td><%= link_to "Delete", product_delete_path(identity), remote: true, class: "btn btn-danger" %></td>
  </tr>
  <% end %>