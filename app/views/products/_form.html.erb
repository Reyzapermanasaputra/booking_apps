<div class="modal-dialog">
	<div class="modal-content">
	<%= form_for @product, remote: true, html: {style: "display:inline;"} do |f| %>
	<div class="modal-body">
		<ul class="errors"></ul>

		<div class="form-group">
			<%= f.label :nama_lembaga, class: "control-label" %>
			<%= f.text_field :nama_lembaga, class: "form-control" %>
		</div>

		<div class="form-group">
			<%= f.label :alamat_lengkap, class: "form-label" %>
			<%= f.text_field :alamat_lengkap, class: "form-control" %>
		</div>

		<div class="form-group">
			<%= f.label :desa, class: "form-label" %>
			<%= f.text_field :desa, class: "form-control" %>
		</div>

		<div class="form-group">
			<%= f.label :kecamatan, class: "form-label" %>
			<%= f.text_field :kecamatan, class: "form-control" %>
		</div>

		<div class="form-group">
			<%= f.label :kabupaten, class: "form-label" %>
			<%= f.text_field :kabupaten, class: "form-control" %>
		</div>

		<div class="form-group">
			<%= f.label :no_akte, class: "form-label" %>
			<%= f.text_field :no_akte, class: "form-control" %>
		</div>
	</div>
	<div class="modal-footer">
		<%= f.submit class: "btn btn-primary" %>
		<%= link_to "Cancel", "#", class: "btn", data: {dismiss: "modal"} %>
	</div>
	<% end %>
	</div>
</div>